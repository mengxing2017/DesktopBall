####### Compile

main.o: ../main.cpp ../ui/desktopball.h \
		../ui/showinfo.h \
		../server/systeminfo.h \
		../server/netspeed.h
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o main.o ../main.cpp

desktopball.o: ../ui/desktopball.cpp ../ui/desktopball.h \
		../ui/showinfo.h \
		../server/systeminfo.h \
		../server/netspeed.h
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o desktopball.o ../ui/desktopball.cpp

netspeed.o: ../server/netspeed.cpp ../server/netspeed.h
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o netspeed.o ../server/netspeed.cpp

systeminfo.o: ../server/systeminfo.cpp ../server/systeminfo.h
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o systeminfo.o ../server/systeminfo.cpp

showinfo.o: ../ui/showinfo.cpp ../ui/showinfo.h \
		../server/systeminfo.h \
		../server/netspeed.h
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o showinfo.o ../ui/showinfo.cpp

qrc_res.o: qrc_res.cpp 
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o qrc_res.o qrc_res.cpp

moc_desktopball.o: moc_desktopball.cpp 
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o moc_desktopball.o moc_desktopball.cpp

moc_showinfo.o: moc_showinfo.cpp 
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o moc_showinfo.o moc_showinfo.cpp

